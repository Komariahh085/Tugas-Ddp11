bangun_datar.keliling_persegi_panjang(6,3)
bangun_datar.luas_belah_ketupat(5,8)
bangun_datar.keliling_belah_ketupat(5)
bangun_datar.luas_jajar_genjang(3,7)
bangun_datar.keliling_jajar_genjang(5,5)

